StartMoving()
{
	bMoving = TRUE;
}

StopMoving()
{
	bMoving = FALSE;
}

Create()
{
	unitviewer = FALSE;
	restore_delay = 2000;
	bMoving = FALSE;
	Static_Var_3 = 0;
	gun_1 = Base;
	gun_2 = Base;
	hide flare1;
	hide flare2;
	dont-cache eye;
	start-script SmokeUnit();
	start-script TexasRanger();
}

SetMaxReloadTime(Func_Var_1)
{
